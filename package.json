{
  "name": "overseas-vue",
  "version": "1.0.0",
  "description": "the project used vue2.5.13 + webpack3.10.0",
  "main": "webpack.config.js",
  "scripts": {
    "dll": "set NODE_ENV=production&&webpack --progress --colors --config ./webpack-dll.config.js",
    "dll-dev": "webpack --progress --colors --config ./webpack-dll.config.js",
    "dev": "webpack-dev-server --progress --config ./webpack.config.js",
    "watch": "webpack --progress --watch",
    "build": "webpack --hide-modules",
    "deploy": "set NODE_ENV=production&&webpack --progress --colors --config ./webpack.config.js",
    "pc_dll": "set PLATFORM=pc&&set NODE_ENV=production&&webpack --progress --colors --config ./webpack-dll.config.js",
    "pc_dll-dev": "set PLATFORM=pc&&webpack --progress --colors --config ./webpack-dll.config.js",
    "pc_watch": "set PLATFORM=pc&&webpack --progress --watch",
    "pc_build": "set PLATFORM=pc&&webpack --hide-modules",
    "pc_deploy": "set PLATFORM=pc&&set NODE_ENV=production&&webpack --bail --progress --colors --config ./webpack.config.js"
  },
  "author": "Jesse",
  "license": "ISC",
