{
    "presets": [
        ["env",
        {
            "modules": false,
            "targets":
            {
                "browsers": ["> 0.2%",
                    "last 5 versions",
                    "Android >= 4.2",
                    "iOS >= 4.3"
                ]
            }
        }],
        "stage-2"
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ]
}
